{
   "author": "Stefan vd",
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "commands": {
      "_execute_page_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+L",
            "mac": "MacCtrl+Shift+L"
         }
      },
      "toggle-feature-nightmode": {
         "description": "__MSG_titelnighttheme__",
         "suggested_key": {
            "default": "Ctrl+Shift+N",
            "mac": "MacCtrl+Shift+N"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "css/light.css" ],
      "js": [ "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "js/fps.js" ],
      "matches": [ "*://*.youtube.com/*" ],
      "run_at": "document_start"
   }, {
      "css": [ "css/youtube.css" ],
      "matches": [ "*://*.youtube.com/*" ]
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://platform.twitter.com https://apis.google.com; img-src *; object-src 'self'",
