{
   "background": {
      "persistent": true,
      "scripts": [ "bg/background.js" ]
   },
   "description": "Confirms when you try to close a pinned tab.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Keep my Pinned Tab",
   "permissions": [ "tabs", "activeTab", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.0"
}
